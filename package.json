{
  "name": "plugin-payid-eliza",
  "version": "0.0.1",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "@elizaos/source": "./src/index.ts",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup --format esm --dts",
    "dev": "tsup --format esm --dts --watch",
    "test": "vitest run",
    "clean": "rm -rf dist",
    "lint": "biome lint .",
    "lint:fix": "biome check --apply .",
    "format": "biome format .",
    "format:fix": "biome format --write .",
    "eliza:test": "npx @elizaos/cli test"
  },
  "dependencies": {
    "@elizaos/cli": "beta",
    "@elizaos/core": "beta",
    "@elizaos/plugin-anthropic": "^1.0.0-beta.41",
    "@elizaos/plugin-bootstrap": "^1.0.0-beta.48",
    "@elizaos/plugin-local-ai": "^1.0.0-beta.48",
    "@elizaos/plugin-openai": "^1.0.0-beta.48",
    "@elizaos/plugin-pdf": "^1.0.0-beta.40",
    "@elizaos/plugin-video-understanding": "^1.0.0-beta.40",
    "axios": "^1.9.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@elizaos/plugin-sql": "1.0.0-beta.48",
    "@langchain/core": "0.0.2",
    "tsup": "8.3.5",
    "typescript": "^5.8.3",
    "vitest": "^3.0.5"
  },
  "peerDependencies": {
    "inversify": "6.2.1",
    "reflect-metadata": "0.2.2",
    "uuid": "11.0.3",
    "zod": "3.24.2"
  },
  "agentConfig": {
    "pluginType": "elizaos:client:1.0.0",
    "pluginParameters": {
      "REVEEL_PAYID_API_KEY": {
        "type": "string",
        "description": "REVEEL_PAYID_API_KEY"
      },
      "REVEEL_PAYID_BASE_URL": {
        "type": "string",
        "description": "REVEEL_PAYID_BASE_URL"
      }
    }
  },
  "packageType": "plugin",
  "platform": "universal",
  "repository": {
    "type": "git",
    "url": "github:metabaronespinosa/plugin-payid-eliza"
  }
}